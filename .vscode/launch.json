{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (web)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/bin/Debug/net7.0/StudentAPI.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "runtimeExecutable": "dotnet",
        "runtimeArgs": [
          "run"
        ],
        "port": 5000,
        "launchBrowser": {
          "enabled": true,
          "args": "${auto-detect-url}",
          "windows": {
            "command": "cmd.exe",
            "args": "/C start ${auto-detect-url}"
          },
          "osx": {
            "command": "open"
          },
          "linux": {
            "command": "xdg-open"
          }
        },
        "environmentVariables": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Views"
        }
      }
    ]
  }
  